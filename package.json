{
  "name": "summarized.dev",
  "scripts": {
    "start": "npm run build:posts-yml && imba -w server.imba",
    "build": "npm run build:posts-yml && npm run build:add-robots && npm run build:add-cname && imba build server.imba",
    "build:posts-yml": "node scripts/build-posts-yml.mjs",
    "build:add-robots": "mkdir -p dist/public && cp src/robots.txt dist/public/robots.txt",
    "build:add-cname": "mkdir -p dist/public && cp src/CNAME dist/public/CNAME"
  },
  "dependencies": {
    "css.gg": "^2.0.0",
    "express": "^4.17.1",
    "imba": "^2.0.0-alpha.146",
    "yaml": "^1.10.2"
  },
  "repository": "friedow/summarized.dev",
  "description": "summarized.dev website including its content",
  "version": "1.0.0",
  "homepage": "https://github.com/friedow/summarized.dev",
  "author": "Author Name (https://github.com/friedow)",
  "license": "MIT"
}
